separator_block_width=15

[mpdstate]
command=scripts/mpdstate
interval=1

[volume]
command=scripts/volume
interval=1

[wifi]
command=scripts/wifi
interval=30
INTERFACE=wlan0

[cpu]
command=scripts/cpu
label=CPU: 
interval=15

[memory]
command=scripts/memory
label=RAM: 
interval=15

[battery]
command=scripts/battery
interval=30

[time]
command=date "+%a %d.%m.%Y %H:%M "
interval=5
